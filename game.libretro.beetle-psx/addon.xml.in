<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.beetle-psx"
		name="PlayStation (Mednafen PSX)"
		version="0.9.39"
		provider-name="Libretro">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
		<import addon="game.controller.ps" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms>PSX</platforms>
		<extensions></extensions>
		<supports_vfs>false</supports_vfs>
		<supports_standalone>false</supports_standalone>
		<supports_keyboard>false</supports_keyboard>
		<supports_mouse>false</supports_mouse>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">PlayStation (Mednafen PSX)</summary>
		<description>Beetle PSX is the PlayStation module of Beetle, a port of Madnafen</description>
		<license>GPLv2</license>
		<platform>@PLATFORM@</platform>
	</extension>
</addon>